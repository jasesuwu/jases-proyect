@model AgroBro.Models.Veterinaria
<div class="card">
  <h5 class="card-header">Datos del Producto</h5>
  <div class="card-body">
      <div asp-validation-summary="ModelOnly"></div>
      <div class="form-group">
        <label asp-for="Nombre"></label><br />
        <span asp-validation-for="Nombre" class="text-danger"></span>
        <input asp-for="Nombre" class="form-control" />
      </div>
      <div class="form-group">
        <label asp-for="Descripcion"></label><br />
        <span asp-validation-for="Descripcion" class="text-danger"></span>
        <input asp-for="Descripcion" class="form-control" />
      </div>
       <div class="form-group">
        <label asp-for="Modo"></label><br />
        <span asp-validation-for="Modo" class="text-danger"></span>
        <input asp-for="Modo" class="form-control" />
      </div>
      <div class="form-group">
        <label asp-for="Aplicabilidad"></label><br />
        <span asp-validation-for="Aplicabilidad" class="text-danger"></span>
        <input asp-for="Aplicabilidad" class="form-control" />
      </div>
      <div class="form-group">
        <label asp-for="Formulacion"></label><br />
        <span asp-validation-for="Formulacion" class="text-danger"></span>
        <input asp-for="Formulacion" class="form-control" />
      </div>
      <div class="form-group">
        <label asp-for="Indicaciones"></label><br>
         <span asp-validation-for="Indicaciones" class="text-danger"></span>
        <input asp-for="Indicaciones" class="form-control"></input>
      </div>
      <div class="form-group">
        <label asp-for="DosisAdm"></label><br>
         <span asp-validation-for="DosisAdm" class="text-danger"></span>
        <input asp-for="DosisAdm" class="form-control"></input>
      </div>
       <div class="form-group">
        <label asp-for="Presentacion"></label><br>
         <span asp-validation-for="Presentacion" class="text-danger"></span>
        <input asp-for="Presentacion" class="form-control"></input>
      </div>
         <div class="form-group">
        <label asp-for="Image"></label><br>
         <span asp-validation-for="Image" class="text-danger"></span>
        <input asp-for="Image" class="form-control"></input>
      </div>
        <div class="form-group">
        <label asp-for="ImageDetail"></label><br>
         <span asp-validation-for="ImageDetail" class="text-danger"></span>
        <input asp-for="ImageDetail" class="form-control"></input>
      </div>
    <button type="submit" class="btn btn-primary">Guardar</button>
  </div>
</div>
